<!DOCTYPE html>
<html>
<head>
  <title>ODCA</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
<link href='https://fonts.googleapis.com/css?family=Raleway' rel='stylesheet' type='text/css'>
<link href='https://fonts.googleapis.com/css?family=Merriweather' rel='stylesheet' type='text/css'>
</head>
<body>
  <div class="container">
    <div id="custom-bootstrap-menu" class="navbar navbar-default navbar-fixed-top" role="navigation">
      <div class="container-fluid">
        <div class="navbar-header"><a class="navbar-brand" href="/"><%= image_tag('logo3.png') %></a>
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-menubuilder"><span class="sr-only">Toggle navigation</span><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
          </button>
        </div>
        <div class="collapse navbar-collapse navbar-menubuilder">
          <ul class="nav navbar-nav navbar-right">
            <li><a href="/educational_services">Educational Programs</a>
            </li>
            <li><a href="/blogs">Blog</a>
            </li>
            <li><a href="/store">Store</a>
            </li>
            <li><a href="/contacts/new">Contact Us</a>
            </li>
            <% if user_signed_in?%>
              <li> <%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
              </li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>



<%= yield %>

  <div class="navbar navbar-default navbar-fixed-bottom bottom">
    <div class="container">

      <ul class="nav navbar-nav navbar-right">
        <li class='facebook'><a href="https://www.facebook.com/Oceans-Daughter-Conservation-Alliance-351791464953076/"><i class="fa fa-facebook-official fa-2x"></i></a>
        </li>
        <li class='twitter'><a href="#"><i class="fa fa-twitter fa-2x"></i></a>
        </li>
      </ul></div>
  </div>
</body>
</html>
